<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<view-state id="ini">
		<transition on="proximo" to="docPersonal"/>
		
		<transition on="backIns" to="finishIni"/>
		<transition on="procesar">
		<render fragments="panelEntity"></render>
		</transition>
	</view-state>
	

	 <subflow-state id="docPersonal" subflow="documentosPersonales">
		<transition on="finishDetail" to="ini"/>
	</subflow-state>

	<end-state id="finishIni" />

</flow>